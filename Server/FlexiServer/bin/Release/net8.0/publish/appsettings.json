{
  "Processes": {
    "Login": {
      "Role": "Login",
      "Host": "http://localhost",
      "Port": 8080,
      "Modules": [ "Login", "CreateAccount" ]
    },
    "Main": {
      "Role": "Main",
      "Host": "http://localhost",
      "Port": 9000,
      "Modules": [ "Room", "Chat" ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
