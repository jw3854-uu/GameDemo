{
  "Processes": {
    "Debug": {
      "Role": "Debug",
      "Host": "http://localhost",
      "Port": 7000,
      "Modules": [ "Login", "CreateAccount","Bag"],
      "UseWebSocket": true
    },
    "Login": {
      "Role": "Login",
      "Host": "http://localhost",
      "Port": 8080,
      "Modules": [ "Login", "CreateAccount" ],
      "UseWebSocket": false
    },
    "Main": {
      "Role": "Main",
      "Host": "http://localhost",
      "Port": 9000,
      "Modules": [ "Room", "Chat" ],
      "UseWebSocket": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
